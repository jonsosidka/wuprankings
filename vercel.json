{
  "version": 2,
  "functions": {
    "api/*.js": {
      "includeFiles": "data/**"
    }
  },
  "routes": [
    { "src": "/api/rankings.available", "dest": "/api/rankings.js" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/fa", "dest": "/public/fa.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}


